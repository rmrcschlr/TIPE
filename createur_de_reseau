liste = []
print("Tapez les noms des arrets (tapez 'end' pour terminer)")
arret = str(input())
while arret != "end" :
    liste.append(arret)
    arret = str(input())

n = len(liste)

matrice = n * [n * [-1]]

for i in range(n) :
    matrice[i][i] = 0

for i in range(n) :
    for j in range(n) :
        if j != i :
            print("Temps de " + liste[i] + " a " + liste[j] + " ? (EntrÃ©e si non)")
            temps = input()
            if temps != '' :
                matrice[i][j] = int(temps)

Reseau = reseau(liste, matrice)

print("Nombre de lignes ?")
nl = int(input())

for i in range(nl) :
    print("Nom de la ligne ?")
    nom = str(input())
    arrets = []
    print("Tapez les noms des arrets de la ligne (tapez 'end' pour terminer)")
    arret = str(input())
    while arret != "end" :
        arrets.append(arret)
        arret = str(input())
    terminus = []
    print("Tapez les noms des terminus de la ligne (tapez 'end' pour terminer)")
    arret = str(input())
    while arret != "end" :
        terminus.append(arret)
        arret = str(input())
    Reseau.ajouter_ligne(nom, arrets, terminus)
